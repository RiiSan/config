    - "duckduckgo.com"
  rewrite:
    - ^https:\/\/duckduckgo.com\/\?q=bd\+([^&]+).+ https://www.baidu.com/s?wd=$1 302
    - ^https:\/\/duckduckgo.com\/\?q=([^&]+).+ https://www.google.com/search?hl=zh-CN&safe=off&newwindow=1&q=$1 302
